{
    "name": "doylee/dphp",
    "description": "DoyleafPHP,A Framework for PHP,By Doylee and Leaf",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "nikic/fast-route": "^1.2",
        "doylee/wafphp": "dev-master"
    },
    "license": "MPL-2.0",
    "authors": [{
        "name": "doylee",
        "email": "dongyunli619@gmail.com"
    }],
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
    "autoload": {
        "psr-4": {
            "Controllers\\": "core/Dphp/controllers/",
            "Models\\": "core/Dphp/models/",
            "Views\\":"core/Dphp/views/"
        },
        "classmap": [
            "app/"
        ],
        "files": [
            "core/Dphp/functions.php"
        ]
          
        
    },
    "config": {
        "vendor-dir": "core/vendor"
    }
}
